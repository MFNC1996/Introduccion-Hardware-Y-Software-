Proceso cap3_3_1
	p<-3.14;
	Escribir "ingrese radio del cilindro";
	leer radio;
	Escribir "ingrese altura del cilindro";
	leer altura;
	area_cilindro<-2*p*radio*(altura+radio);
	volumen_cilindro<-p*(radio*radio)*altura;
	Escribir "el area del cilindro es:",area_cilindro;
	Escribir "el volumen del cilindro es:",volumen_cilindro;
FinProceso
